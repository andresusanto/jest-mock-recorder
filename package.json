{
  "name": "jest-mock-recorder",
  "version": "1.2.0",
  "description": "A tool to record and replay jest mocks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andresusanto/jest-mock-recorder.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andresusanto/jest-mock-recorder/issues"
  },
  "homepage": "https://github.com/andresusanto/jest-mock-recorder#readme",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/npm": "^8.0.2",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "jest": "27.2.2",
    "ts-jest": "27.0.5",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "jest": ">=27.2.2",
    "ts-jest": ">=27.0.5"
  },
  "dependencies": {
    "fs-extra": "^10.0.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
